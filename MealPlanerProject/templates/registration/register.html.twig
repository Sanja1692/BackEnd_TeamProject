{% extends 'base.html.twig' %}

{% block title %}Register{% endblock %}

{% block body %}

    <div class="split2 d-flex align-items-center justify-content-center">



    {{ form_start(registrationForm) }}
 
 

            {{ form_row(registrationForm.fname) }}
        {{ form_row(registrationForm.lname) }}
        {{ form_row(registrationForm.picture) }}
        {{ form_row(registrationForm.email) }}
        {{ form_row(registrationForm.plainPassword, {
            label: 'Password'
        }) }}
        {{ form_row(registrationForm.agreeTerms) }}

        <button type="submit" class="btn eddt2">Register</button>
    {{ form_end(registrationForm) }}
    
    
    
    </div>
{% endblock %}
